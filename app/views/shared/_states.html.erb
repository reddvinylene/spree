<% if @states.length == 0 %>
  <div class="text">
    <%= text_field("#{presenter_type}_presenter", :address_state_name, :autocomplete => "off") %>
  </div>
<% else %>
  <div class="select">
    <%= collection_select("#{presenter_type}_presenter", :address_state_id, @states, :id, :name, {:include_blank => true}) %>
  </div>
<% end %>
