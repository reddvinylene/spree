<% form_for [:admin, @search], :url => "" do |f| %>
  <% f.fields_for @search.conditions do |s| %>
    <label><%= t :date_range %></label>
    <div class="sub">
      <%= s.calendar_date_select :created_at_after %>
      <label><%= t :start %></label>
    </div>
    <div class="sub">
      <%= s.calendar_date_select :created_at_before %>
      <label><%= t :stop %></label>
    </div>
    <div class="button"><%= button t(:search) %></div>
  <% end %>
<% end %>
